{
  "name": "aex",
  "version": "0.1.0",
  "description": "Small scripts for Adobe After Effecrts",
  "scripts": {
    "build": "cross-env NODE_WEBPACK_MINIMIZE=yes webpack",
    "watch": "cross-env NODE_WEBPACK_MINIMIZE=no webpack --watch",
    "fmt": "prettier --write /src/**/*.ts",
    "typedoc": "typedoc --mode file --out doc ./src/main.ts",
    "gendocs": "typedoc --out ./docs/ --target ES3 --tsconfig ./tsconfig.json --exclude node_modules --ignoreCompilerErrors src/"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "ae-to-json": "^1.1.2",
    "after-effects": "^0.4.15",
    "concat-files": "^0.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "estktap": "^0.1.6",
    "jasminejsx": "0.0.4",
    "prettier": "^1.19.1",
    "tape": "^4.12.1",
    "ts-loader": "^5.4.3",
    "typescript": "^3.7.4",
    "typescript-eslint": "0.0.1-alpha.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "private": true,
  "dependencies": {
    "@types/fuzzyset": "^1.0.2",
    "@types/node": "^12.12.11",
    "JSON2": "^0.1.0",
    "es5-shim": "^4.5.13",
    "extendscript-es5-shim-ts": "0.0.1",
    "fuzzy": "^0.1.3",
    "fuzzyset.js": "0.0.8",
    "js-search": "^1.4.3",
    "typedoc": "^0.15.5",
    "types-for-adobe": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nasikusa/aex.git"
  },
  "keywords": [
    "AE",
    "AfterEffects",
    "Adobe",
    "AdobeAfterEffects",
    "ExtendScript",
    "TypeScript"
  ],
  "author": "youkan",
  "license": "MIT"
}
